{
    "name": "kartik-v/yii2-google-docs",
    "description": "Yii2 library to work with Google Docs API.",
    "keywords": [
        "yii2",
        "extension",
        "google",
		"docs",
		"gdocs",
        "word"
    ],
    "homepage": "https://github.com/kartik-v/yii2-google-docs",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Kartik Visweswaran",
            "email": "kartikv2@gmail.com",
            "homepage": "http://www.krajee.com/"
        }
    ],
    "require": {
        "google/apiclient": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "kartik\\gdocs\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}